		// TODO Auto-generated method stub
		
		ArrayList<Board> boardMovieList=null;
		ArrayList<Board> boardRevieList=null;
		ArrayList<Board> boardEventList=null;
		
		
		
		
		
		
		request.setAttribute("b_division", b_division);
